{
    "name": "codicastudio/cookie-consent",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/cookie-consent",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com",
            "homepage": "https://codicastudio.dev",
            "role": "Developer"
        }
    ],

    "require": {
        "php" : "^7.2",
        "illuminate/support": "^7.0",
        "illuminate/view": "^7.0",
        "illuminate/cookie": "^7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "codicastudio\\CookieConsent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudio\\CookieConsent\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\CookieConsent\\CookieConsentServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
